{
  "buildOptions": {
    "additionalArguments": [
      "/ruleset:../../build/Analyzers.ruleset"
    ],
    "keyFile": "../../Autofac.snk",
    "nowarn": [
      "CS1591"
    ],
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "dependencies": {
    "Autofac": "4.0.1",
    "Moq": "4.6.38-alpha",
    "Microsoft.CodeAnalysis.FxCopAnalyzers": {
      "type": "build",
      "version": "1.1.0"
    },
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "System.Runtime.Analyzers": {
      "type": "build",
      "version": "1.1.0"
    }
  },
  "description": "Autofac extension for automocking and creation of mock objects in Moq.",
  "frameworks": {
    "net45": {},
    "netstandard1.3": {}
  },
  "packOptions": {
    "iconUrl": "https://cloud.githubusercontent.com/assets/1156571/13684110/16b8f152-e6bf-11e5-84ae-22c66c6d351a.png",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "owners": [
      "Autofac Contributors"
    ],
    "projectUrl": "http://autofac.org",
    "releaseNotes": "Release notes are at https://github.com/autofac/Autofac.Extras.Moq/releases",
    "repository": {
      "type": "git",
      "url": "https://github.com/autofac/Autofac.Extras.Moq"
    },
    "requireLicenseAcceptance": false,
    "summary": "The Moq integration package allows you to automatically create mock dependencies for both concrete and mock abstract instances in unit tests using an Autofac container. ",
    "tags": [
      "autofac",
      "di",
      "ioc",
      "dependencyinjection",
      "moq"
    ]
  },
  "version": "4.1.0-rc4-*"
}
